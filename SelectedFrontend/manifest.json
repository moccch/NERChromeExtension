{
    "manifest_version": 3,
  
    "name": "NER",
    "description": "This extension highlights Name Entity",
    "version": "1.0",
    "icons": {
		"128": "images/icon128.png",
		"48": "images/icon48.png",
		"16": "images/icon16.png"
    },
  
    "action": {
		"default_icon": "images/icon16.png",
		"default_popup": "popup.html"
    },

	"host_permissions": ["<all_urls>"],

	"permissions": [
        "tabs",
        "scripting"
    ]
	
}